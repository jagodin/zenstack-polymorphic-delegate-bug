datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["rhel-openssl-3.0.x", "native"]
}

model Media {
  id                 Int    @id() @default(autoincrement())
  title              String
  mediaType          String
  delegate_aux_movie Movie?
}

model Movie {
  id                 Int      @id()
  director           Director @relation(fields: [directorId], references: [id])
  directorId         Int
  duration           Int
  rating             String
  delegate_aux_media Media    @relation(fields: [id], references: [id], onDelete: Cascade, onUpdate: Cascade)
}

model Director {
  id     Int     @id() @default(autoincrement())
  name   String
  email  String
  movies Movie[]
}
